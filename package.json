{
  "name": "timecards",
  "description": "Bob Sponja time cards as service",
  "homepage": "https://nicedoc.io/Kikobeats/timecards",
  "version": "0.0.0",
  "main": "src/index.js",
  "bin": {
    "timecards": "bin/server"
  },
  "author": {
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/timecards.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/timecards/issues"
  },
  "keywords": [
    "microservice",
    "bob",
    "sponja",
    "timecards"
  ],
  "dependencies": {
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "debug-logfmt": "~1.0.4",
    "express": "~4.17.1",
    "file-extension": "~4.0.5",
    "got": "~10.4.0",
    "helmet": "~3.21.2",
    "mime-types": "~2.1.26",
    "morgan": "~1.9.1",
    "unique-random-array": "~2.0.0"
  },
  "devDependencies": {
    "@microlink/mql": "latest",
    "git-dirty": "latest",
    "json-future": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "svr": "latest",
    "untracked": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "scripts",
    "src"
